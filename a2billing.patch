+function isLuhnNum($num)
+{
+	$length = strlen($num);
+	$tot = 0;
+	for($i=$length-1;$i>=0;$i--)
+	{
+		$digit = substr($num, $i, 1);
+		if ((($length - $i) % 2) == 0)
+		{
+			$digit = $digit*2;
+			if ($digit>9)
+			{
+				$digit = $digit-9;
+			}
+		}
+		$tot += $digit;
+	}
+	return (($tot % 10) == 0);
+}
