+function arguments($argv) {
+	$_ARG = array ();
+	array_shift($argv); //skip argv[0] !
+	foreach ($argv as $arg) {
+		if (preg_match('/--([^=]+)=(.*)/', $arg, $reg)) {
+			$_ARG[$reg[1]] = $reg[2];
+		}
+		elseif (preg_match('/--([^=]+)/', $arg, $reg)) {
+			$_ARG[$reg[1]] = true;
+		}
+		elseif (preg_match('/^-([a-zA-Z0-9])/', $arg, $reg)) {
+			$_ARG[$reg[1]] = true;
+		} else {
+			$_ARG['input'][] = $arg;
+		}
+	}
+	return $_ARG;
