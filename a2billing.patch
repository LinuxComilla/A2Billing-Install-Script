+function MDP_STRING($chrs = LEN_CARDNUMBER)
+{
+	$pwd = "";
+	mt_srand((double) microtime() * 1000000);
+	while (strlen($pwd) < $chrs) {
+		$chr = chr(mt_rand(0, 255));
+		if (preg_match("/^[0-9a-z]$/i", $chr))
+			$pwd = $pwd . $chr;
+	};
+	return strtolower($pwd);
+}
